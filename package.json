{
  "name": "picimpact",
  "private": true,
  "author": "Bess Croft <besscroft@foxmail.com>",
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "dev:server": "next dev",
    "dev:turbopack": "pnpm run dev:prisma && pnpm run prisma:seed && next dev --turbopack",
    "dev:prisma": "pnpm run prisma:dev && pnpm run prisma:generate",
    "build": "next build",
    "build:analyzer": "ANALYZE=true next build",
    "build:vercel": "pnpm run prisma:deploy && pnpm run prisma:generate && pnpm run prisma:seed && next build",
    "build:netlify": "pnpm run prisma:deploy && pnpm run prisma:generate && pnpm run prisma:seed && next build",
    "build:node": "pnpm run prisma:deploy && pnpm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"app/**/*.{ts,tsx}\" --ignore-pattern \"components/ui/*.{tsx}\"",
    "prisma:format": "npx prisma format",
    "prisma:generate": "npx prisma generate",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:seed": "npx prisma db seed",
    "prebuild": "npx prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
